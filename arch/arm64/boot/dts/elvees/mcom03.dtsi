			clock-names = "clk_xin", "clk_ahb";
			clocks = <&hsperiph_ucg1 HSP_SDMMC0_XIN_CLK>,
				 <&hsperiph_ucg0 HSP_SDMMC0_CLK>;
			resets = <&hsperiph_reset HSPERIPH_RST_SDMMC0>;
			elvees,ctrl-id = <0>;
			status = "disabled";

